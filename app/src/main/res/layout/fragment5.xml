<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/content"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        >

        <!--TextView style="?android:textAppearanceMedium"

            android:lineSpacingMultiplier="1.2"
            android:padding="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="#000"
            android:text="Q1 Do you think if the same information was presented as follows, it will be easier to understand? Here, we have just converted the numbers reported earlier to what kind of a quality of experience you can expect on that network performance." />

        <CheckBox
            android:id="@+id/cb2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Satisfied"
            android:textColor="#00f"
            android:checked="false"
            android:textSize="20sp"/>

        <CheckBox
            android:id="@+id/cb3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Dissatisfied"
            android:textColor="#00f"
            android:checked="false"
            android:textSize="20sp"/>

        <CheckBox
            android:id="@+id/cb4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Neither satisfied nor dissatisfied"
            android:textColor="#00f"
            android:checked="false"
            android:textSize="20sp"/-->


        <TextView style="?android:textAppearanceMedium"
            android:id="@+id/f5tv1"
            android:lineSpacingMultiplier="1.2"
            android:padding="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="#000"
            android:text="Q5 Here is a list of statements that describe the shown Label. Describe your opinion." />

        <LinearLayout
            android:id="@+id/f5ll1"
            android:orientation="vertical"
            android:layout_below="@+id/f5tv1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView style="?android:textAppearanceMedium"

                android:lineSpacingMultiplier="1.2"
                android:padding="10dp"
                android:textColor="#000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1. Label was easy to understand	" />

            <CheckBox
                android:id="@+id/f5q5s1cb1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Agree"
                android:textColor="#00f"
                android:checked="false"
                android:textSize="20sp"/>

            <CheckBox
                android:id="@+id/f5q5s1cb2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Disagree"
                android:textColor="#00f"
                android:checked="false"
                android:textSize="20sp"/>

            <CheckBox
                android:id="@+id/f5q5s1cb3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="NA"
                android:textColor="#00f"
                android:checked="false"
                android:textSize="20sp"/>


        </LinearLayout>



        <LinearLayout
            android:id="@+id/f5ll2"
            android:layout_below="@+id/f5ll1"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView style="?android:textAppearanceMedium"

                android:lineSpacingMultiplier="1.2"
                android:padding="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000"
                android:text="2. I found everything that I need in the label " />

            <CheckBox
                android:id="@+id/f5q5s2cb1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Agree"
                android:textColor="#00f"
                android:checked="false"
                android:textSize="20sp"/>

            <CheckBox
                android:id="@+id/f5q5s2cb2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Disagree"
                android:textColor="#00f"
                android:checked="false"
                android:textSize="20sp"/>

            <CheckBox
                android:id="@+id/f5q5s2cb3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="NA"
                android:textColor="#00f"
                android:checked="false"
                android:textSize="20sp"/>


        </LinearLayout>

        <LinearLayout
            android:id="@+id/f5ll3"
            android:layout_below="@+id/f5ll2"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView style="?android:textAppearanceMedium"

                android:lineSpacingMultiplier="1.2"
                android:padding="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000"
                android:text="3. There is not enough information	" />

            <CheckBox
                android:id="@+id/f5q5s3cb1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Agree"
                android:textColor="#00f"
                android:checked="false"
                android:textSize="20sp"/>

            <CheckBox
                android:id="@+id/f5q5s3cb2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Disagree"
                android:textColor="#00f"
                android:checked="false"
                android:textSize="20sp"/>

            <CheckBox
                android:id="@+id/f5q5s3cb3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="NA"
                android:textColor="#00f"
                android:checked="false"
                android:textSize="20sp"/>


        </LinearLayout>

        <LinearLayout
            android:id="@+id/f5ll4"
            android:layout_below="@+id/f5ll3"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView style="?android:textAppearanceMedium"
                android:padding="10dp"
                android:textColor="#000"
                android:lineSpacingMultiplier="1.2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="4. There should be a standardised Label format	" />

            <CheckBox
                android:id="@+id/f5q5s4cb1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Agree"
                android:textColor="#00f"
                android:checked="false"
                android:textSize="20sp"/>

            <CheckBox
                android:id="@+id/f5q5s4cb2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Disagree"
                android:textColor="#00f"
                android:checked="false"
                android:textSize="20sp"/>

            <CheckBox
                android:id="@+id/f5q5s4cb3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="NA"
                android:textColor="#00f"
                android:checked="false"
                android:textSize="20sp"/>


        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_below="@+id/f5ll4"
            android:layout_marginTop="5px"
            android:gravity="center_horizontal|bottom"
            android:paddingTop="2px" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="20px"
                android:paddingRight="20px"
                android:text=" Swipe >>"/>
        </LinearLayout>

    </RelativeLayout>
</ScrollView>